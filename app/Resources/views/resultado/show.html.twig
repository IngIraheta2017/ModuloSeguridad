{% extends 'base.html.twig' %}

{% block body %}
    <h1>Resultado</h1>

    <table>
        <tbody>
            <tr>
                <th>Descripcionresultados</th>
                <td>{{ resultado.descripcionResultados }}</td>
            </tr>
            <tr>
                <th>Idresultado</th>
                <td>{{ resultado.idResultado }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('resultado_index') }}">Back to the list</a>
