{% extends 'base.html.twig' %}

{% block body %}
    <h1>Asignacions list</h1>

    <table>
        <thead>
            <tr>
                <th>Idasignacion</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for asignacion in asignacions %}
            <tr>
                <td><a href="{{ path('asignacion_show', { 'idAsignacion': asignacion.idAsignacion }) }}">{{ asignacion.idAsignacion }}</a></td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('asignacion_show', { 'idAsignacion': asignacion.idAsignacion }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('asignacion_edit', { 'idAsignacion': asignacion.idAsignacion }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('asignacion_new') }}">Create a new asignacion</a>
        </li>
    </ul>
{% endblock %}
